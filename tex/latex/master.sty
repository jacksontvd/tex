%ENVIRONMENTS
%environments get:
%	definition
%	crefname (theorems, lemmas, propositions, and conjectures are always capitalized.
%	Everything else is controlled by cref)
%	numberwithin setting(in cls files) (e.g. 
%\numberwithin{equation}{chapter})
%	Snippet
\newtheorem{thm}{Theorem}
\crefname{thm}{Theorem}{Theorems}
\newtheorem*{thm*}{Theorem}
%
\newtheorem{lem}[thm]{Lemma}
\crefname{lem}{Lemma}{Lemmata}
\newtheorem*{lem*}{Lemma}
%
\newtheorem{prop}[thm]{Proposition}
\crefname{prop}{Proposition}{Propositions}
\newtheorem*{prop*}{Proposition}
%
\newtheorem{cor}[thm]{Corollary}
\crefname{cor}{Corollary}{Corollaries}
\newtheorem*{cor*}{Corollary}
%
\newtheorem{con}{Conjecture}
\crefname{con}{Conjecture}{Conjectures}
\newtheorem*{con*}{Conjecture}
%
\newtheorem{fact}{Fact}
\Crefname{fact}{Fact}{Facts}
%
\newtheorem{obs}{Observation}
\Crefname{obs}{Observation}{Observations}
%%%
\theoremstyle{definition}
\newtheorem{defn}{Definition}
\Crefname{defn}{Definition}{Definitions}
%
\newtheorem{exr}{Exercise}
\Crefname{exr}{Exercise}{Exercises}
%
\newtheorem*{sol}{Solution}
% \Crefname{sol}{Solution}{Solutions}
%
\newtheorem{exm}{Example}
\Crefname{exm}{Example}{Examples}
%
\newtheorem{cexm}{Counterexample}
\Crefname{cexm}{Counterexample}{Counterexamples}
%
\newtheorem{wrn}{Warning}
\Crefname{wrn}{Warning}{Warnings}
%
\newtheorem{qn}{Question}
\Crefname{qn}{Question}{Questions}
%
\newtheorem{clm}{Claim}
\Crefname{clm}{Claim}{Claims}
\newtheorem{proof_clm}{Claim}[thm]
% proof_clm is for claims inside of a proof. So they are numbered within theorems
%%%
\theoremstyle{remark}
\newtheorem{rmk}{Remark}
\Crefname{rmk}{Remark}{Remarks}
\newtheorem*{rmk*}{Remark}
%
\newtheorem{note}{Note}
\Crefname{note}{Note}{Notes}
\newtheorem*{note*}{Note}
%
\newtheorem*{imp}{Important}
%
\newtheorem{dig}{Digression}
\Crefname{dig}{Digression}{Digressions}
%
\newtheorem{joke}{Joke}
\Crefname{joke}{Joke}{Jokes}
%
\newtheorem{idea}{Idea}
\Crefname{idea}{Idea}{Ideas}
%%%
\Crefname{axiom}{Axiom}{Axioms}
%%%
\crefformat{footnote}{#2\footnotemark[#1]#3}
%%%
%%% custom environments
% i.e. environments where you can give it any label you like (this is useful for claim 1, claim 2, claim 2', claim 3)
% exm: 
% \begin{custom_clm}{2'}
% custom must be referenced with ref, i.e. it must have the type in the label. This
% explains why I made a
% new environment for customclm rather than just typing claim into custom whenever I want
% a custom label on a claim, because I want cref to
% still control its capitalization.
%% IMPORTANT: Do not use this to reference named theorems (e.g. Zorn's lemma). 
%There is \nameref for this!
\newtheorem{innercustom}{}
\newenvironment{custom}[1]
  {\renewcommand\theinnercustom{#1}\innercustom}
  {\endinnercustomthm}
%
\newtheorem{innercustomclm}{Claim}
\newenvironment{custom_clm}[1]
  {\renewcommand\theinnercustomclm{#1}\innercustomclm}
  {\endinnercustomclm}
\Crefname{innercustomclm}{Claim}{Claims}
